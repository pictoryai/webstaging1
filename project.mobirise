          "_name": "ampFooter23",
          "_customHTML": "<section class=\"ampFooter23\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <!--  -->\n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"showReverse\">\n        <!--  -->\n        <header>Columns:</header>\n        <select title=\"Title:\" name=\"alignTitle\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>1 column:</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn1\" checked>\n        <input type=\"checkbox\" title=\"Show Logo:\" name=\"showLogo\">\n        <input type=\"range\" title=\"Logo Width\" inline name=\"logoWidth\" min=\"55\" max=\"100\" value=\"100\" step=\"1\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Icon:\" name=\"showIcons\" checked>\n        <input type=\"range\" title=\"Icons:\" name=\"countSocialIcon\" min=\"0\" max=\"6\" step=\"1\" value=\"4\" condition=\"showIcons\">\n        <input type=\"range\" title=\"Size Icon:\" name=\"sizeIcon\" min=\"1\" max=\"2\" step=\"0.1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Color Icon:\" name=\"colorIcon\" value=\"#3756c0\" condition=\"showIcons\">\n        <input type=\"color\" title=\"BG Color Icon:\" name=\"colorBgIcon\" value=\"#000000\" condition=\"showIcons\">\n        <!--  -->\n        <header>2 column:</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn2\" checked>\n        <input type=\"range\" title=\"Strings:\" name=\"countStringCol1\" min=\"0\" max=\"8\" step=\"1\"