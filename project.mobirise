l1\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n        <select title=\"Align:\" name=\"alignCol1\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>3 column:</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn3\" checked=\"\">\n        <input type=\"range\" title=\"Amount string:\" name=\"countStringCol2\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n        <select title=\"Align\" name=\"alignCol2\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>4 column:</header>\n        <input type=\"checkbox\" title=\"Show column\" name=\"showColumn4\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn4\" condition=\"showColumn4\" checked=\"\">\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image:\" value=\"@PROJECT_PATH@/assets/images/background4.jpg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#000000\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.05\" value=\"0.85\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <